{
    "name": "ffi-headers/headers-contracts",
    "type": "library",
    "description": "PHP FFI headers base library",
    "license": "MIT",
    "keywords": ["ffi", "headers", "contracts", "abstractions", "decoupling", "interfaces", "interoperability", "standards"],
    "support": {
        "source": "https://github.com/php-ffi-headers/headers-contracts",
        "issues": "https://github.com/php-ffi-headers/headers-contracts/issues",
        "docs": "https://github.com/php-ffi-headers/headers-contracts/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Nesmeyanov Kirill",
            "email": "nesk@xakep.ru",
            "homepage": "https://serafimarts.ru",
            "role": "maintainer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "FFI\\Contracts\\Headers\\": "src"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.22",
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "FFI\\Contracts\\Headers\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
